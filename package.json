{
  "name": "docs",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "test": "yarn run lint && yarn run prettier-diff && yarn run build",
    "deploy": "yarn run build && gh-pages -d _book",
    "lint": "tslint -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "prettier": "prettier --single-quote --trailing-comma es5 'src/**/*.{ts,tsx,css,md}'",
    "prettier-diff": "yarn prettier --list-different",
    "format": "yarn prettier --write"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    }
  },
  "devDependencies": {
    "@accounts/tslint-config-accounts": "0.0.6",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "gatsby": "1.9.199",
    "gatsby-plugin-react-helmet": "2.0.5",
    "gatsby-plugin-react-next": "1.0.9",
    "gatsby-plugin-sass": "1.0.17",
    "gatsby-plugin-sitemap": "1.2.13",
    "gatsby-plugin-styled-components": "2.0.6",
    "gatsby-plugin-typescript": "1.4.16",
    "gatsby-plugin-typography": "1.7.14",
    "gatsby-remark-autolink-headers": "1.4.12",
    "gatsby-remark-prismjs": "1.2.15",
    "gatsby-source-filesystem": "1.5.19",
    "gatsby-transformer-remark": "1.7.32",
    "gh-pages": "1.1.0",
    "prettier": "1.10.2",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "bulma": "0.6.2",
    "gatsby-link": "1.6.37",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "styled-components": "3.1.6",
    "typography": "0.16.6",
    "typography-theme-doelger": "0.15.11"
  }
}
