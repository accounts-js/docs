{
  "name": "docs",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "test": "yarn run lint && yarn run prettier-diff && yarn run build",
    "deploy": "yarn run build && gh-pages -d _book",
    "lint": "tslint -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "prettier": "prettier --single-quote --trailing-comma es5 'src/**/*.{ts,tsx,css,md}'",
    "prettier-diff": "yarn prettier --list-different",
    "format": "yarn prettier --write"
  },
  "devDependencies": {
    "@accounts/tslint-config-accounts": "0.0.6",
    "@types/react": "16.0.34",
    "@types/react-dom": "16.0.3",
    "gatsby": "1.9.159",
    "gatsby-plugin-react-helmet": "1.0.8",
    "gatsby-plugin-react-next": "1.0.7",
    "gatsby-plugin-sass": "1.0.15",
    "gatsby-plugin-styled-components": "1.0.5",
    "gatsby-plugin-typescript": "1.4.12",
    "gatsby-plugin-typography": "1.7.10",
    "gatsby-remark-autolink-headers": "1.4.11",
    "gatsby-remark-prismjs": "1.2.12",
    "gatsby-source-filesystem": "1.5.11",
    "gatsby-transformer-remark": "1.7.28",
    "gh-pages": "0.12.0",
    "prettier": "1.10.2",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "dependencies": {
    "bulma": "0.6.2",
    "gatsby-link": "1.6.34",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "styled-components": "2.4.0",
    "typography": "0.16.6",
    "typography-theme-doelger": "0.15.11"
  }
}
